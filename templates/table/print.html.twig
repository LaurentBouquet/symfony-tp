{% extends 'base.html.twig' %}

{% block title %}Hello TableController!{% endblock %}

{% block body %}

<div class="example-wrapper">
    <h1>Table de {{ n }}</h1>
    <table>
        {# {% for i in 0..10 %}
            <tr><td>{{ i }} x {{ n }} = </td><td> {{ i*n }} </td></tr>
        {% endfor %} #}        
        {% set index = 0 %}
        {% for value in values %}
            <tr><td>{{ index }} = </td><td> {{ value }} </td></tr>
            {% set index = index + 1 %}
        {% endfor %}
    </table>

</div>
{% endblock %}
